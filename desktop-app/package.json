{
  "name": "client",
  "version": "0.0.1",
  "main": "main.js",
  "scripts": {
    "start": "npm run build && node build/main.js",
    "build": "babel src -s -d build",
    "postinstall": "shx cp -n .env.sample .env",
    "dev": "concurrently -r -k 'gulp' 'nodemon src/main.js --watch src --exec babel-node'",
    "bdd": "mocha --compilers coffee:coffee-script/register --require source-map-support/register",
    "test": "npm run build && npm run bdd"
  },
  "dependencies": {
    "babel-cli": "^6.7.7",
    "babel-preset-es2015": "^6.6.0",
    "babel-preset-stage-3": "^6.5.0",
    "commander": "^2.9.0",
    "googleapis": "^7.1.0",
    "idb-plus-blob-store": "^1.0.0",
    "ipfs": "^0.4.5",
    "ipfs-api": "^3.0.0",
    "openurl": "^1.1.1",
    "readline": "^1.3.0",
    "recursive-readdir": "^2.0.0",
    "winston": "^2.2.0",
    "ws": "^1.1.0"
  },
  "devDependencies": {
    "chai": "^3.5.0",
    "coffee-script": "^1.10.0",
    "concurrently": "^2.1.0",
    "dotenv": "^2.0.0",
    "gulp": "^3.9.1",
    "gulp-eslint": "^2.0.0",
    "mocha": "^2.5.3",
    "nodemon": "^1.9.2",
    "shx": "^0.1.2",
    "source-map-support": "^0.4.0"
  }
}
